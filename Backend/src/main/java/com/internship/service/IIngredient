package com.internship.service;

import com.internship.entity.Ingredient;
import java.util.List;

public interface IIngredient {
    List<Ingredient> GetAllIngredient();

    Ingredient FindIngredient(int ingredientId);

    Ingredient Create(Ingredient objIngredient);

    Ingredient update(int ingredientId, Ingredient objIngredient);

    void Delete(int ingredientId);
}